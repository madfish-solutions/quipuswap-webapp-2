    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "swap-router-sdk": "1.15.0-beta",
    "swr": "^1.3.0",
    "throttle-debounce": "^5.0.0",
    "url": "^0.11.0",
    "use-debounce": "^8.0.1",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/amplitude-js": "^8.16.1",
