{
  "name": "quipuswap-webapp",
  "version": "3.0.2",
  "private": true,
  "scripts": {
    "dev:mainnet": "cross-env REACT_APP_NETWORK=mainnet REACT_APP_MAINNET_BASE_URL=http://localhost:3000 REACT_APP_HANGZHOUNET_BASE_URL=http://localhost:3002 REACT_APP_ITHACANET_BASE_URL=http://localhost:3003 PORT=3000 craco start",
    "dev:hangzhounet": "cross-env REACT_APP_NETWORK=hangzhounet REACT_APP_MAINNET_BASE_URL=http://localhost:3000 REACT_APP_HANGZHOUNET_BASE_URL=http://localhost:3002 REACT_APP_ITHACANET_BASE_URL=http://localhost:3003 PORT=3002 craco start",
    "dev:ithacanet": "cross-env REACT_APP_NETWORK=ithacanet REACT_APP_MAINNET_BASE_URL=http://localhost:3000 REACT_APP_HANGZHOUNET_BASE_URL=http://localhost:3002 REACT_APP_ITHACANET_BASE_URL=http://localhost:3003 PORT=3003 craco start",
    "pre-build": "node -v && npm -v && yarn -v && node ./scripts/build.js",
    "clear": "node ./scripts/clear.js && rm -rf ./node_modules",
    "build": "yarn pre-build && craco build",
    "start": "yarn dev:mainnet",
    "test": "craco test",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .ts,.tsx",
    "eject": "react-scripts eject",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "^2.3.12",
    "@craco/craco": "^6.4.3",
    "@quipuswap/sdk": "^3.0.0",
    "@taquito/beacon-wallet": "^12.0.3",
    "@taquito/taquito": "^12.0.0",
    "@temple-wallet/dapp": "^6.1.1",
    "@tippyjs/react": "^4.2.6",
    "@types/react-modal": "^3.13.1",
    "assert": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "constate": "^3.3.0",
    "craco-alias": "^3.0.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.0",
    "final-form": "^4.20.6",
    "formik": "^2.2.9",
    "https-browserify": "^1.0.0",
    "mem": "^9.0.2",
    "memoizee": "^0.4.15",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-countdown-hook": "^1.1.1",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.8",
    "react-modal": "^3.14.4",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "react-select": "^5.2.2",
    "react-slick": "^0.28.1",
    "react-table": "^7.7.0",
    "react-textarea-autosize": "^8.3.3",
    "react-toastify": "^8.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "swr": "^1.2.2",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/memoizee": "^0.4.7",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-slick": "^0.23.8",
    "eslint": "^8.4.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix && yarn tsc && yarn run build"
    }
  }
}
