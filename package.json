    "@taquito/beacon-wallet": "^12.1.1",
    "@taquito/taquito": "^12.1.1",
    "@temple-wallet/dapp": "^7.0.0",
    "@tippyjs/react": "^4.2.6",
    "@types/throttle-debounce": "^5.0.0",
    "amplitude-js": "^8.18.2",
    "assert": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "constate": "^3.3.2",
    "craco-alias": "^3.0.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "cypress": "^9.6.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "dotenv": "^16.0.1",
    "final-form": "^4.20.7",
    "formik": "^2.2.9",
    "https-browserify": "^1.0.0",
    "md5": "^2.3.0",
    "mem": "^9.0.2",
    "memoizee": "^0.4.15",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.4.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-countdown-hook": "^1.1.1",
    "react-dom": "^18.1.0",
    "react-final-form": "^6.5.9",
    "react-modal": "^3.15.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
