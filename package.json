{
  "name": "quipuswap-webapp",
  "version": "3.1.2",
  "private": true,
  "scripts": {
    "pre-build": "node -v && npm -v && yarn -v && node ./scripts/build.js",
    "start": "yarn pre-build && craco start",
    "build": "yarn pre-build && craco build",
    "clear": "node ./scripts/clear.js && rm -rf ./node_modules",
    "test": "craco test",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .ts,.tsx",
    "e2e": "npx cypress run --record --key $(grep CYPRESS_KEY .env | cut -d '=' -f2)",
    "eject": "react-scripts eject",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "^3.1.0",
    "@craco/craco": "^6.4.3",
    "@quipuswap/sdk": "^3.0.0",
